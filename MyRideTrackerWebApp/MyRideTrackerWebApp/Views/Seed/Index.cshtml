@model IEnumerable<Ride>
@{
    ViewData["Title"] = "Seed Data";
}

<h3>Seed Data</h3>
<form method="post">
    <div class="row">
        <div class="col-md-3">
            <div class="form-group">
                <label>Number of Objects to Create</label>
                <input class="form-control" name="count" value="50" />
            </div>
        </div>  
    </div>
    <div>
        <button type="submit" asp-action="CreateProductionData"
                class="btn btn-outline-primary">
            Production Seed
        </button>
        <button asp-action="ClearData" class="btn btn-danger">
            Clear Database
        </button>
    </div>
</form>
<h5 class="m-2">
    There are @ViewBag.Count rides in the database
</h5>






